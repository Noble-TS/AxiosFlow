{
  "name": "axiosflow",
  "version": "0.1.1",
  "private": false,
  "description": "AxiosFlow is a powerful TypeScript library that simplifies API consumption. It uses decorators and reflection to automatically generate type-safe API clients and types for your backend APIs, reducing boilerplate code and enhancing development efficiency.",
  "keywords": [
    "typescript",
    "api",
    "api-client",
    "api-client-generator",
    "type-safe",
    "decorators",
    "reflection",
    "axios",
    "axios-client",
    "axios-helper",
    "react",
    "express",
    "angular"
  ],
  "homepage": "https://github.com/Noble-TS/AxiosFlow#readme",
  "bugs": {
    "url": "https://github.com/Noble-TS/AxiosFlow/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Noble-TS/AxiosFlow.git"
  },
  "license": "MIT",
  "author": "Noble-Ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "axiosflow": "dist/index.js"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "cli": "node --loader ts-node/esm src/index.ts",
    "generate": "node ./dist/index.js generate"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/reflect-metadata": "^0.1.0",
    "axios": "^1.7.9",
    "axios-mock-adapter": "^1.19.0",
    "axiosflow": "^0.1.0",
    "commander": "^9.2.0",
    "cra-template-typescript": "1.2.0",
    "dotenv": "^16.4.7",
    "jest-mock-axios": "^4.8.0",
    "react": "^19.0.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "reflect-metadata": "^0.2.2",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-typescript": "^7.26.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/prettier": "^3.0.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "axios": "^1.7.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.4.2",
    "rollup": "^3.29.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^4.9.5"
  },
  "module": "./dist/index.js",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
  }
}
