{
  "name": "axiosflow",
  "version": "0.1.6",
  "private": false,
  "description": "AxiosFlow is a powerful TypeScript library that simplifies API consumption. It uses decorators and reflection to automatically generate type-safe API clients and types for your backend APIs, reducing boilerplate code and enhancing development efficiency.",
  "keywords": [
    "typescript",
    "api",
    "api-client",
    "api-client-generator",
    "type-safe",
    "decorators",
    "reflection",
    "axios",
    "axios-client",
    "axios-helper",
    "react",
    "express",
    "angular"
  ],
  "homepage": "https://github.com/Noble-TS/AxiosFlow#readme",
  "bugs": {
    "url": "https://github.com/Noble-TS/AxiosFlow/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Noble-TS/AxiosFlow.git"
  },
  "license": "MIT",
  "author": "Noble-Ts",
  "type": "module",
  "exports": {
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs",
      "types": "./dist/src/cli/index.d.ts"
    }
  },
  "main": "./dist/cli.cjs",
  "types": "./dist/src/cliindex.d.ts",
  "bin": {
    "axiosflow": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "cli": "node --loader ts-node/esm src/cli/index.ts",
    "generate": "node ./dist/cli.cjs generate"
  },
  "dependencies": {
    "@types/reflect-metadata": "^0.1.0",
    "axios": "^1.7.9",
    "commander": "^9.2.0",
    "dotenv": "^16.4.7",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
 
    "@types/node": "^22.10.2",
    "@types/prettier": "^3.0.0",
    "axios": "^1.7.9",
   
    "prettier": "^3.4.2",
       "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.18.2"
  },
  "peerDependencyRules": {
    "ignoreMissing": [
      "@types/express"
    ]
  },
  "module": "./dist/index.js",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
  
}
